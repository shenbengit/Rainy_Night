<?xml version="1.0" encoding="utf-8"?>

<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置填充颜色 为白色 -->
    <solid android:color="#C6FDD1" />

    <!-- 设置几何图形四个角的弧度 -->
    <corners android:radius="200dp"/>

    <!-- 设置几何形状绘制边框 红色边框 -->
