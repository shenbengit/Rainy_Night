<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10.0dip" />
            <gradient
                android:angle="270.0"
                android:centerColor="#ff1e4276"
                android:centerY="0.45"
                android:endColor="#ff1e4276"
                android:startColor="#ff1e4276" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="10.0dip" />
                <gradient
                    android:angle="270.0"
                    android:centerColor="#ff1e4276"
                    android:centerY="0.45"
                    android:endColor="#ff1e4276"
                    android:startColor="#ff1e4276" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10.0dip" />
                <gradient
                    android:angle="270.0"
                    android:centerColor="#ff35b1ff"
                    android:centerY="0.45"
                    android:endColor="#ff35b1ff"
                    android:startColor="#ff35b1ff" />
            </shape>
        </clip>
    </item>
</layer-list>